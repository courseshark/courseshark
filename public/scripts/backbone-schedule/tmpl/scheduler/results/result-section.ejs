<div class="results-section">
  <div class="row-fluid">
    <div class="prof span18"><%= prof %></div>
    <div class="friend-slot span2">
      <div class="friend-icon" title="You have a friend in this section">
        <div class="icon-container">
          <svg viewBox="0 0 32 22" version="1.1" xmlns="http://www.w3.org/2000/svg">
            <path class="fill" fill="#000000" d=" M 9.63 0.00 L 13.45 0.00 C 18.22 2.81 15.26 8.94 14.83 13.19 C 17.69 14.49 20.42 16.06 23.08 17.71 C 23.03 19.14 22.97 20.57 22.92 22.00 L 0.00 22.00 L 0.00 17.69 C 2.61 15.99 5.37 14.54 8.17 13.18 C 7.71 8.92 4.72 2.72 9.63 0.00 Z" />
            <path class="fill" fill="#000000" d=" M 19.81 5.99 C 20.15 2.19 26.54 1.78 27.36 5.50 C 27.82 8.26 28.45 11.02 30.25 13.25 C 28.77 13.98 27.25 14.60 25.71 15.17 C 26.76 17.77 30.14 17.94 32.00 19.83 L 32.00 22.00 L 24.14 22.00 C 24.01 20.45 23.98 18.88 23.63 17.36 C 21.98 15.25 19.25 14.44 16.99 13.17 C 18.64 11.08 19.56 8.62 19.81 5.99 Z" />
          </svg>
        </div>
      </div>
    </div>
    <div class="seats span4"></div>
  </div>
  <div class="row-fluid">
    <div class="span19 row-2">
      <div class="row-fluid">
        <div class="section-id span24"><%= section_id %></div>
      </div>
      <div class="row-fluid">
        <div style="min-height:20px">
          <div class="days span24">
            <div class="day span2" id="monday">m</div>
            <div class="day span2" id="tuesday">t</div>
            <div class="day span2" id="wednesday">w</div>
            <div class="day span2" id="thursday">th</div>
            <div class="day span2" id="friday">f</div>
          </div>
        </div>
      </div>
    </div>
    <div class="span5">
      <button class="make-notification" title="Watch this section for a seat to open">
        <i class="icon-bullhorn icon-white"></i>
      </button>
      <button class="add" title="Add this class to your schedule"></button>
    </div>
  </div>
  <div class="details">
    <div class="row-fluid">
      <div class="description span24">
        <%= description %>
      </div>
    </div>
    <div class="row-fluid">
      <div class="hours span24">
        <%= hours %> Credit Hours
      </div>
    </div>
    <div class="row-fluid friends-row" style="display: none">
      <div class="section-friends span24">
        Friends:
      </div>
    </div>
  </div>
  <div class="row-fluid">
    <div class="expander span24">
      <i class="icon-chevron-down"></i>
    </div>
  </div>
</div>
