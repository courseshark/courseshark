<div class="wk-event-listing" style="top:<%=top_offset%>px;height:<%=height%>px;border-top-color:<%=color%>">
	<dl class="wk-event-listing-dl">
		<dt data-color="<%=color%>">
			<%=section.name%>
			<small>[<%=section.number%>]</small>
		</dt>
		<dd>
			<div class="row-fluid">
				<div class="span24">
					<%=slot.instructor%>
				</div>
			</div>
			<div class="row-fluid">
				<div class="span24">
					<%=slot.location%>
				</div>
			</div>
			<div class="row-fluid">
				<div class="span24">
					<span class="timeslot-time">
						<%=startHourAdjusted%>:<%=slot.startTime.getUTCMinutes()%>-<%=endHourAdjusted%>:<%=slot.endTime.getUTCMinutes()%>
					</span>
				</div>
			</div>
			
		</dd>
	</dl> 
</div>