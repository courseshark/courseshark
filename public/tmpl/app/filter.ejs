<div class="filter-panel">
	<div class="row-fluid">
		<div class="span24">
			<div class="search">
				<h1><%= search %></h1>
				<input type="text" id="search" name="search"/>
			</div>
		</div>
		<div class="advanced-search">
			<h1><%= adv_search %></h1>
		</div>
	</div>
	<div class="attribute">
		<div class="row-fluid">	
			<h5>day of week</h5>
			<div class="days span24">
				<div class="day span2" id="monday">
					<label for="m">m</label>
					<input class="daybox" type="checkbox" name="day" id="monday" value="m"/>
				</div>
				<div class="day span2" id="tuesday">
					<label for="t">t</label>
					<input class="daybox" type="checkbox" name="day" id="tuesday" value="t"/>
				</div>
				<div class="day span2" id="wednesday">
					<label for="w">w</label>
					<input class="daybox" type="checkbox" name="day" id="wednesday" value="w"/>
				</div>
				<div class="day span2" id="thursday">
					<label for="th">th</label>
					<input class="daybox" type="checkbox" name="day" id="thursday" value="th"/>
				</div>
				<div class="day span2" id="friday">
					<label for="f">f</label>
					<input class="daybox" type="checkbox" name="day" id="friday" value="f"/>
				</div>
			</div>
		</div>
	</div>
	<div class="row-fluid">
		<div class="attribute">
			<h5>time</h5>
			<div class="slider"></div>
		</div>
	</div>
</div>
