<h1>Users <small>found <%=users.length%></h1>

<div class="row">
	<div class="span1">
		<i>Search</i>
	</div>
	<div class="span9">
		<form name="user" action="#" method="get">
			<input type="text" name="user.firstName" placeholder="First Name"/>
			<input type="text" name="user.lastName" placeholder="Last Name"/>
			<input type="email" name="user.email" placeholder="Email"/>
			<input type="text" name="user.oauth" placeholder="oauth"/>
			<select name="user.admin">
				<option value="">admin?</option>
				<option value="true">Yes</option>
				<option value="false">No</option>
			</select>
			<button type="submit">go</button>
		</form>
	</div>
</div>



<div class="row">
	<div class="span10">
		<table class="table">
			<thead>
				<tr>
					<th>ID</th>
					<th>Name</th>
					<th>Email</th>
					<th>Oauth Prov.</th>
					<th>School</th>
					<th># Friends</th>
					<th>Admin</th>
				</tr>
			</thead>
			<tbody>
		<% if (users.length) { %>
    	<% users.forEach(function(user){ %>
				<tr>
					<td><%=user._id%></td>
					<td><%=user.name%></td>
					<td><%=user.email?user.email:'&mdash;'%></td>
					<td><%=user.oauth?user.oauth:'&mdash;'%></td>
					<td><%=user.school?user.school.name:'&mdash;'%></td>
					<td><%=user.friends.length%></td>
					<td><%=user.admin%></td>
				</tr>
			<% }) %>
		<% } %>
			</tbody>
		</table>
	</div>
</div>